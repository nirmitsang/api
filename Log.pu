        """
        if self.collection is None:
            _log('error', "Not connected to MongoDB")
            return None
        
        try:
            # Clean array notation from field name
            cleaned_field = field_name.replace('[]', '')
            
            # Build query
            query = {
                cleaned_field: {
                    '$exists': True,
                    '$ne': None,
